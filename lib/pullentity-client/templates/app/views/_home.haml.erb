= section({:id=>"home"}) do

  = header do
    = link_to "#aside-left" , "data-target" => "aside" do
      = icon :reorder

    %h1 <%= app_name %>
    = link_to "#first-page", "data-target" => "section" do
      = icon :home

    = link_to "#aside-top", "data-target" => "aside" do
      = icon :search

  = footer("data-type" => "toolbar") do
    %a.onlyIcon
      = icon(:refresh)
    .content
      %h2
        Generated at:
        = Time.now
    = link_to "#aside-right", "data-target" => "aside", :class=>"onlyIcon" do
      = icon(:edit)

  = article( { :id => 'first-page', :class => 'scrollable' }) do
    = list_view(:id=>"someid") do
      <pre> run: pullentity view s list &lt;model&gt; &lt;collection&gt;</pre>
